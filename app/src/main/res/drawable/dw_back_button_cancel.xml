<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/ML_Dialog"/>
            <corners android:radius="@dimen/_14sdp"/>
            <stroke android:color="@color/ML_Dialog" android:width="@dimen/_1sdp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/ML_White"/>
            <corners android:radius="@dimen/_14sdp"/>
            <stroke android:color="@color/ML_Dialog" android:width="@dimen/_1sdp"/>
        </shape>
    </item>

</selector>
