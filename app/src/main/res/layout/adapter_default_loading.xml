<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_2sdp"
    android:background="@drawable/dw_back_recycler_loading"
    android:padding="@dimen/_5sdp">


    <ir.mlcode.latifiarchitecturelibrary.customs.ML_EditText
        android:id="@+id/ML_EditText2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_100sdp"
        android:padding="@dimen/_5sdp"
        app:delimiterBack="@color/ML_Dialog"
        app:delimiterWidth="1px"
        app:delimiter_marginLeft="@dimen/_5sdp"
        app:delimiter_marginRight="@dimen/_5sdp"
        app:image_height="@dimen/_10sdp"
        app:image_width="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:normalBack="@drawable/dw_back_recycler_loading_items" />


    <ir.mlcode.latifiarchitecturelibrary.customs.ML_EditText
        android:id="@+id/ml_EditTextSubstitute"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_150sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:padding="@dimen/_5sdp"
        app:delimiterBack="@color/ML_Dialog"
        app:delimiterWidth="1px"
        app:delimiter_marginLeft="@dimen/_5sdp"
        app:delimiter_marginRight="@dimen/_5sdp"
        app:image_height="@dimen/_10sdp"
        app:image_width="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ML_EditText2"
        app:normalBack="@drawable/dw_back_recycler_loading_items" />

    <ir.mlcode.latifiarchitecturelibrary.customs.ML_EditText
        android:id="@+id/ML_EditText3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:padding="@dimen/_5sdp"
        app:delimiterBack="@color/ML_Dialog"
        app:delimiterWidth="1px"
        app:delimiter_marginLeft="@dimen/_5sdp"
        app:delimiter_marginRight="@dimen/_5sdp"
        app:image_height="@dimen/_10sdp"
        app:image_width="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ml_EditTextSubstitute"
        app:normalBack="@drawable/dw_back_recycler_loading_items" />


    <ir.mlcode.latifiarchitecturelibrary.customs.ML_EditText
        android:id="@+id/ML_EditText4"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5sdp"
        android:padding="@dimen/_5sdp"
        app:delimiterBack="@color/ML_Dialog"
        app:delimiterWidth="1px"
        app:delimiter_marginLeft="@dimen/_5sdp"
        app:delimiter_marginRight="@dimen/_5sdp"
        app:image_height="@dimen/_10sdp"
        app:image_width="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ML_EditText3"
        app:normalBack="@drawable/dw_back_recycler_loading_items" />


</androidx.constraintlayout.widget.ConstraintLayout>



